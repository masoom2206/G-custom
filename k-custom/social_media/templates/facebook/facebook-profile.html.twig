 <div class="facebook-profile-block bg-f2f2f2">
  <div class="col-lg-12 col-xs-12 col-sm-12">
    <div class="facebook-block-wraper">
      <div class="facebook-profile-banner">
        <div class="profile-banner"> 
           {% if data.settings_field.Banner.value is defined %}
             <div class="profile-banner-img" style="background: url({{data.settings_field.Banner.value}}); background-position: center;
                  background-repeat: no-repeat;
                  background-size: cover;height:200px">
             </div>
          {% else %}
          <div class="profile-banner-img">
          </div> {% endif %} </div>
          <div class="profile-pic">
          <img src="{{data.settings_field.Profile.value}}" class="rounded-circle img-responsive" alt="pic">
         </div>
        </div>
        <div class="facebook-profile-content">
          
          <b>About: </b>{{data.settings_field.About.value}}<br>
          <b>Phone: </b>{{data.settings_field.Phone.value}}<br>
          <b>Emails: </b>{{data.settings_field.Emails.value}}<br>
          
        </div>
       </div> 
     </div>  
   </div> 
