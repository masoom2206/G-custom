
<div class="my-kaboodles" id="my-shared-kaboodles">
  <!--shared kaboodles list-->
  <div class="kaboodles-mid-block">
    <div class="kaboodles-container d-flex w-100 ff-rw">
      <!-- tiles - this will be in react, probably uses a flex grid -->
      {% if data %}
        {% for node in data %}
        <div class="kaboodle">
          <div class="offset-1">
            <div class="card">
              <div class="card">
                <!--<a class="delete-shared-kaboodle tooltip-text" href="javascript:void(0)" id="shared-{{ node.nid }}-{{ uid }}" title="Delete"></a>-->
                <a href="{{ path('my_kaboodles.kaboodle_dashboard_page', {'node': node.nid}) }}"><img class="cover-image" width="100%" src="{{ node.img }}" /></a>
              </div>
              <div class="card-body text-center kaboodle-title"><a href="{{ path('my_kaboodles.kaboodle_dashboard_page', {'node': node.nid}) }}">{{ node.title }}</a></div>
              <div class="card-body type-icon text-center">
                <!-- <span class="icon-left"><hr/></span>
                <span class="icon-center"><img width="16" src="{{ node.icon }}" /> {{ node.tname }}</span>
                <span class="icon-right"><hr/></span> -->
                <div class="kaboodle-shared-by text-center">Shared by: <a href="{{ path('entity.user.contact_form', {'user': node.owner_id}) }}">{{ node.sharedby }}</a></div>
              </div>
            </div>
          </div>
        </div>
        {% endfor %}
      {% else %}
        <div class="col-lg-12">
          <div class="default_message d-flex ai-center text-center">
            <span>You currently have no shared Kaboodles. <a href="{{ path('entity.node.canonical', {'node': 198}) }}">{{ 'Learn more'|t }}</a>.</span>
          </div>
        </div>
      {% endif %}
    </div>
  </div>
</div>
<div class="my-kaboodles-pager">{{pager}}</div>
