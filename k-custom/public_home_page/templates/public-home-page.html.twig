      <div class="homepage-media">
        {{ field_home_page_slide|raw }}
      </div>
    </div>
  </div>
</div>
<div class="homepage-bottom text-center" style="margin-bottom:0 padding: 0px !important;">
  <a href="#"><img src="https://s3.us-west-2.amazonaws.com/kaboodlemedia.com/s3fs_public/site-images/2020/down-arrow-lines-white.png"></a>
